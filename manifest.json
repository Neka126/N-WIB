{
  "name": "Ez Unblock",
  "manifest_version": 2,
  "version": "22.1",
  "description": "Easily unblock websites!",
  "background": {
      "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": {
      "16": "icons/iconWeb.png",
      "32": "icons/iconWeb.png"
    }
}}